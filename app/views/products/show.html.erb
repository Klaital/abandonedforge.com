  <div class="product-description">
    <h2><%= @product.name %></h2>
    <p><%= @product.description %></p>
    <div class="product-imageset">
    <% @product.images.each do |image| %>
      <div class="product-image">
        <img src="<%= image.large_url %>" />
      </div>
    <% end %>
    </div>
  </div>